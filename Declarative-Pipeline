pipeline{
         agent any
                
          stages{
                stage("git checkout")
                 {
                   steps{
                         git credentialsId: 'github', url: 'https://github.com/lakshmikar23/Webapplication.git'
                       }
                  }
 
           stage("maven build")
                   {
                    steps{
                         def mvnHOME = tool name: 'maven 3.6.0', type: 'maven'
                         def mvnCMD = "${mvnHOME}/bin/mvn"
                         sh "${mvnCMD} clean package"
                         
                         }
                    }
               }
           }    
   
                    
